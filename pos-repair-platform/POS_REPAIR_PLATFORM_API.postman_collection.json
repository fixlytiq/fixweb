	"item": [
		{
			"name": "Authentication",
			"item": [
				{
					"name": "Register",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"// Auto-save token and store ID from registration response",
									"// Check if environment is selected",
									"if (!pm.environment.name) {",
									"    console.log('⚠️ WARNING: No environment selected! Token will not be saved.');",
									"    console.log('Please select an environment from the top-right dropdown.');",
									"}",
									"",
									"if (pm.response.code === 201) {",
									"    try {",
									"        const response = pm.response.json();",
									"        ",
									"        // Save token",
									"        if (response.token) {",
									"            pm.environment.set('authToken', response.token);",
									"            console.log('✅ Token saved to environment:', pm.environment.name || 'default');",
									"            console.log('Token preview:', response.token.substring(0, 20) + '...');",
									"        } else {",
									"            console.log('⚠️ No token in response. Response keys:', Object.keys(response));",
									"        }",
									"        ",
									"        // Save store ID",
									"        if (response.store && response.store.id) {",
									"            pm.environment.set('storeId', response.store.id);",
									"            console.log('✅ Store ID saved:', response.store.id);",
									"        } else {",
									"            console.log('⚠️ No store ID in response');",
									"        }",
									"        ",
									"        // Verify token was saved",
									"        const savedToken = pm.environment.get('authToken');",
									"        if (savedToken) {",
									"            console.log('✅ Verification: Token is now in environment');",
									"        } else {",
									"            console.log('❌ ERROR: Token was not saved! Check environment selection.');",
									"        }",
									"    } catch (e) {",
									"        console.log('❌ Error parsing response:', e.message);",
									"        console.log('Response body:', pm.response.text());",
									"    }",
									"} else {",
									"    console.log('⚠️ Registration failed with status:', pm.response.code);",
									"    console.log('Response:', pm.response.text());",
									"}"
								],
								"type": "text/javascript"
							}
						}
					],
					"request": {
						"auth": {
							"type": "noauth"
						},
						"method": "POST",
						"header": [
							{
								"key": "Content-Type",
								"value": "application/json"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n  \"ownerName\": \"John Doe\",\n  \"storeName\": \"Main Street Repair Shop\",\n  \"storeEmail\": \"store@example.com\",\n  \"pin\": \"1234\"\n}"
						},
						"url": {
							"raw": "{{baseUrl}}/auth/register",
							"host": ["{{baseUrl}}"],
							"path": ["auth", "register"]
						}
					},
					"response": []
				},
				{
					"name": "Login",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"// Auto-save token and store ID from login response",
									"// Check if environment is selected",
									"if (!pm.environment.name) {",
									"    console.log('⚠️ WARNING: No environment selected! Token will not be saved.');",
									"    console.log('Please select an environment from the top-right dropdown.');",
									"}",
									"",
									"if (pm.response.code === 200) {",
									"    try {",
									"        const response = pm.response.json();",
									"        ",
									"        // Save token",
									"        if (response.token) {",
									"            pm.environment.set('authToken', response.token);",
									"            console.log('✅ Token saved to environment:', pm.environment.name || 'default');",
									"            console.log('Token preview:', response.token.substring(0, 20) + '...');",
									"        } else {",
									"            console.log('⚠️ No token in response. Response keys:', Object.keys(response));",
									"            console.log('Full response:', JSON.stringify(response, null, 2));",
									"        }",
									"        ",
									"        // Save store ID",
									"        if (response.user && response.user.storeId) {",
									"            pm.environment.set('storeId', response.user.storeId);",
									"            console.log('✅ Store ID saved:', response.user.storeId);",
									"        } else {",
									"            console.log('⚠️ No store ID in response');",
									"        }",
									"        ",
									"        // Verify token was saved",
									"        const savedToken = pm.environment.get('authToken');",
									"        if (savedToken) {",
									"            console.log('✅ Verification: Token is now in environment');",
									"        } else {",
									"            console.log('❌ ERROR: Token was not saved! Check environment selection.');",
									"        }",
									"    } catch (e) {",
									"        console.log('❌ Error parsing response:', e.message);",
									"        console.log('Response body:', pm.response.text());",
									"    }",
									"} else {",
									"    console.log('⚠️ Login failed with status:', pm.response.code);",
									"    console.log('Response:', pm.response.text());",
									"}"
								],
								"type": "text/javascript"
							}
						}
					],
					"request": {
						"auth": {
							"type": "noauth"
						},
						"method": "POST",
						"header": [
							{
								"key": "Content-Type",
								"value": "application/json"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n  \"storeEmail\": \"store@example.com\",\n  \"pin\": \"1234\"\n}"
						},
						"url": {
							"raw": "{{baseUrl}}/auth/login",
							"host": ["{{baseUrl}}"],
							"path": ["auth", "login"]
						}
					},
					"response": []
				}
			]
